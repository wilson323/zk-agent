      cov_1d3ud3b2c2().s[15]++;
      console.log('✅ Database disconnected successfully');
    } catch (error) {
      /* istanbul ignore next */
      cov_1d3ud3b2c2().s[16]++;
      console.error('❌ Database disconnection failed:', error);
      /* istanbul ignore next */
      cov_1d3ud3b2c2().s[17]++;
      throw error;
    }
  }
  static async healthCheck() {
    /* istanbul ignore next */
    cov_1d3ud3b2c2().f[3]++;
    cov_1d3ud3b2c2().s[18]++;
    try {
      /* istanbul ignore next */
      cov_1d3ud3b2c2().s[19]++;
      await DatabaseService.getInstance().$queryRaw`SELECT 1`;
      /* istanbul ignore next */
      cov_1d3ud3b2c2().s[20]++;
      return true;
    } catch (error) {
      /* istanbul ignore next */
      cov_1d3ud3b2c2().s[21]++;
      console.error('❌ Database health check failed:', error);
      /* istanbul ignore next */
      cov_1d3ud3b2c2().s[22]++;
      return false;
    }
  }
  static async runTransaction(callback) {
    /* istanbul ignore next */
    cov_1d3ud3b2c2().f[4]++;
    const prisma =
    /* istanbul ignore next */
    (cov_1d3ud3b2c2().s[23]++, DatabaseService.getInstance());
    /* istanbul ignore next */
    cov_1d3ud3b2c2().s[24]++;
    return await prisma.$transaction(callback);
  }
}
