/**
 * 创建分页响应
 */
function createPaginatedResponse(data, total, page, limit) {
  /* istanbul ignore next */
  cov_1zbhjkna1b().f[7]++;
  cov_1zbhjkna1b().s[64]++;
  return {
    items: data,
    pagination: {
      page,
      limit,
      total,
      totalPages: Math.ceil(total / limit),
      hasNext: page * limit < total,
      hasPrev: page > 1
    }
  };
}
/**
 * 标准化错误处理
 */
function handleApiError(error) {
  /* istanbul ignore next */
  cov_1zbhjkna1b().f[8]++;
  cov_1zbhjkna1b().s[65]++;
  if (error instanceof zod_1.z.ZodError) {
    /* istanbul ignore next */
    cov_1zbhjkna1b().b[20][0]++;
    cov_1zbhjkna1b().s[66]++;
    return ApiResponseWrapper.error(ErrorCode.VALIDATION_ERROR, '数据验证失败', error.errors);
  } else
  /* istanbul ignore next */
  {
    cov_1zbhjkna1b().b[20][1]++;
  }
  cov_1zbhjkna1b().s[67]++;
  if (error instanceof Error) {
    /* istanbul ignore next */
    cov_1zbhjkna1b().b[21][0]++;
    cov_1zbhjkna1b().s[68]++;
    return ApiResponseWrapper.error(ErrorCode.INTERNAL_SERVER_ERROR, error.message, process.env.NODE_ENV === 'development' ?
    /* istanbul ignore next */
    (cov_1zbhjkna1b().b[22][0]++, error.stack) :
    /* istanbul ignore next */
    (cov_1zbhjkna1b().b[22][1]++, undefined), 500);
  } else
  /* istanbul ignore next */
  {
    cov_1zbhjkna1b().b[21][1]++;
  }
  cov_1zbhjkna1b().s[69]++;
  return ApiResponseWrapper.error(ErrorCode.INTERNAL_SERVER_ERROR, '未知错误', undefined, 500);
}
