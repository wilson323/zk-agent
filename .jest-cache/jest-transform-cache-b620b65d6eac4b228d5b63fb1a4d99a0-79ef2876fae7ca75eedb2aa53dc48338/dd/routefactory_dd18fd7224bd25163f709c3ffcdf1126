      return ApiResponseWrapper.error(ErrorCode.INTERNAL_SERVER_ERROR, '内部服务器错误', process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_1zbhjkna1b().b[16][0]++, error instanceof Error ?
      /* istanbul ignore next */
      (cov_1zbhjkna1b().b[17][0]++, error.message) :
      /* istanbul ignore next */
      (cov_1zbhjkna1b().b[17][1]++, error)) :
