      /* istanbul ignore next */
      cov_11l0m5nfhm().f[21]++;
      cov_11l0m5nfhm().s[76]++;
      return service.health.status === 'healthy';
    }).sort((a, b) => {
      /* istanbul ignore next */
      cov_11l0m5nfhm().f[22]++;
      // 基于延迟和错误率排序
      const scoreA =
      /* istanbul ignore next */
      (cov_11l0m5nfhm().s[77]++, a.health.latency + a.health.errorRate * 10);
      const scoreB =
      /* istanbul ignore next */
      (cov_11l0m5nfhm().s[78]++, b.health.latency + b.health.errorRate * 10);
      /* istanbul ignore next */
      cov_11l0m5nfhm().s[79]++;
      return scoreA - scoreB;
    }));
    /* istanbul ignore next */
    cov_11l0m5nfhm().s[80]++;
    if (aiServices.length === 0) {
      /* istanbul ignore next */
      cov_11l0m5nfhm().b[15][0]++;
      cov_11l0m5nfhm().s[81]++;
      throw new Error('No healthy AI services available');
    } else
    /* istanbul ignore next */
    {
      cov_11l0m5nfhm().b[15][1]++;
    }
    cov_11l0m5nfhm().s[82]++;
    return aiServices[0].provider;
  }
