  /**
   * 智能负载均衡
   */
  async getOptimalAIProvider() {
    /* istanbul ignore next */
    cov_11l0m5nfhm().f[18]++;
    const aiServices =
    /* istanbul ignore next */
    (cov_11l0m5nfhm().s[73]++, Array.from(this.services.entries()).filter(([name]) => {
      /* istanbul ignore next */
      cov_11l0m5nfhm().f[19]++;
      cov_11l0m5nfhm().s[74]++;
      return name.startsWith('ai-');
    }).map(([name, health]) => {
      /* istanbul ignore next */
