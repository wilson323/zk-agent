    /* istanbul ignore next */
    (cov_11l0m5nfhm().s[107]++, 0);
    /* istanbul ignore next */
    cov_11l0m5nfhm().s[108]++;
    for (const [key, value] of this.resourceCache.entries()) {
      /* istanbul ignore next */
      cov_11l0m5nfhm().s[109]++;
      if (
      /* istanbul ignore next */
      (cov_11l0m5nfhm().b[21][0]++, value.expiry) &&
      /* istanbul ignore next */
      (cov_11l0m5nfhm().b[21][1]++, now > value.expiry)) {
        /* istanbul ignore next */
        cov_11l0m5nfhm().b[20][0]++;
        cov_11l0m5nfhm().s[110]++;
        this.resourceCache.delete(key);
        /* istanbul ignore next */
        cov_11l0m5nfhm().s[111]++;
        cleared++;
      } else
      /* istanbul ignore next */
      {
        cov_11l0m5nfhm().b[20][1]++;
      }
    }
    /* istanbul ignore next */
    cov_11l0m5nfhm().s[112]++;
    if (cleared > 0) {
      /* istanbul ignore next */
      cov_11l0m5nfhm().b[22][0]++;
      cov_11l0m5nfhm().s[113]++;
