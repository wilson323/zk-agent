      validateBody: zod_1.z.object({
        content: zod_1.z.string().min(1).max(4000),
        type: zod_1.z.enum(['text', 'file', 'image', 'system']).default('text'),
        role: zod_1.z.enum(['user', 'assistant', 'system', 'tool']).default('user'),
        metadata: zod_1.z.record(zod_1.z.unknown()).optional()
      }),
      ...config
    });
