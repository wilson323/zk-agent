            cov_v5tyq9woc().b[9][0]++;
            cov_v5tyq9woc().s[37]++;
            return handleValidationError(paramsResult.error, config);
          } else
          /* istanbul ignore next */
          {
            cov_v5tyq9woc().b[9][1]++;
          }
          cov_v5tyq9woc().s[38]++;
          validatedRequest.validatedParams = paramsResult.data;
        } else
        /* istanbul ignore next */
        {
          cov_v5tyq9woc().b[7][1]++;
        }
        // 调用原始处理函数
        cov_v5tyq9woc().s[39]++;
        return await handler(validatedRequest, context);
      } catch (error) {
        /* istanbul ignore next */
        cov_v5tyq9woc().s[40]++;
        if (config.debug) {
          /* istanbul ignore next */
          cov_v5tyq9woc().b[10][0]++;
          cov_v5tyq9woc().s[41]++;
          console.error('[ValidationMiddleware] 验证中间件错误:', error);
        } else
        /* istanbul ignore next */
        {
          cov_v5tyq9woc().b[10][1]++;
        }
        cov_v5tyq9woc().s[42]++;
        if (error instanceof ValidationError) {
          /* istanbul ignore next */
          cov_v5tyq9woc().b[11][0]++;
          cov_v5tyq9woc().s[43]++;
          return handleValidationError(error, config);
        } else
        /* istanbul ignore next */
        {
          cov_v5tyq9woc().b[11][1]++;
        }
        // 处理其他错误
        cov_v5tyq9woc().s[44]++;
        return server_1.NextResponse.json({
          error: '服务器内部错误',
          message: error instanceof Error ?
          /* istanbul ignore next */
          (cov_v5tyq9woc().b[12][0]++, error.message) :
          /* istanbul ignore next */
          (cov_v5tyq9woc().b[12][1]++, '未知错误')
        }, {
          status: 500
        });
      }
    };
  };
}
// 📝 命名规范：验证工具函数
function validateWithSchema(schema, data, context) {
  /* istanbul ignore next */
  cov_v5tyq9woc().f[4]++;
  cov_v5tyq9woc().s[45]++;
  try {
    const validData =
    /* istanbul ignore next */
    (cov_v5tyq9woc().s[46]++, schema.parse(data));
    /* istanbul ignore next */
    cov_v5tyq9woc().s[47]++;
    return {
      success: true,
      data: validData
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_v5tyq9woc().s[48]++;
    if (error instanceof zod_1.z.ZodError) {
      /* istanbul ignore next */
      cov_v5tyq9woc().b[13][0]++;
      cov_v5tyq9woc().s[49]++;
      return {
        success: false,
        error: new ValidationError(`${context} validation failed`, error.issues.map(issue => {
          /* istanbul ignore next */
          cov_v5tyq9woc().f[5]++;
          cov_v5tyq9woc().s[50]++;
          return {
            path: issue.path,
            message: issue.message,
            code: issue.code
          };
        }), 400)
      };
    } else
    /* istanbul ignore next */
    {
      cov_v5tyq9woc().b[13][1]++;
    }
    cov_v5tyq9woc().s[51]++;
    return {
      success: false,
      error: new ValidationError(`${context} validation error`, [{
        path: [],
        message: 'Unknown validation error',
        code: 'unknown'
      }], 400)
    };
  }
}
// 📝 命名规范：错误处理函数
function handleValidationError(error, config) {
  /* istanbul ignore next */
  cov_v5tyq9woc().f[6]++;
  cov_v5tyq9woc().s[52]++;
  // 如果有自定义错误处理器，使用它
  if (config.onValidationError) {
    /* istanbul ignore next */
    cov_v5tyq9woc().b[14][0]++;
    cov_v5tyq9woc().s[53]++;
    return config.onValidationError(error);
  } else
  /* istanbul ignore next */
  {
    cov_v5tyq9woc().b[14][1]++;
  }
  // 默认错误响应
  cov_v5tyq9woc().s[54]++;
  return server_1.NextResponse.json({
    error: error.message,
    issues: error.issues,
    timestamp: new Date().toISOString()
  }, {
    status: error.statusCode
  });
}
// 📝 命名规范：安全JSON解析函数
async function safeParseJSON(request) {
  /* istanbul ignore next */
  cov_v5tyq9woc().f[7]++;
  cov_v5tyq9woc().s[55]++;
  try {
    const text =
    /* istanbul ignore next */
    (cov_v5tyq9woc().s[56]++, await request.text());
    /* istanbul ignore next */
    cov_v5tyq9woc().s[57]++;
    if (!text.trim()) {
      /* istanbul ignore next */
      cov_v5tyq9woc().b[15][0]++;
      cov_v5tyq9woc().s[58]++;
      return {};
    } else
    /* istanbul ignore next */
    {
      cov_v5tyq9woc().b[15][1]++;
    }
    cov_v5tyq9woc().s[59]++;
    return JSON.parse(text);
  } catch (error) {
    /* istanbul ignore next */
    cov_v5tyq9woc().s[60]++;
    throw new ValidationError('Invalid JSON in request body', [{
      path: ['body'],
      message: 'Request body must be valid JSON',
      code: 'invalid_json'
    }]);
  }
}
