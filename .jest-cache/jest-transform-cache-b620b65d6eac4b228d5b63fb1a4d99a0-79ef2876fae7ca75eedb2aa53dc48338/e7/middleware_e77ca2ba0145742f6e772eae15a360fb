// 📝 命名规范：安全增强中间件
function withSecurity(config) {
  /* istanbul ignore next */
  cov_v5tyq9woc().f[12]++;
  cov_v5tyq9woc().s[66]++;
  return function securityMiddleware(handler) {
    /* istanbul ignore next */
    cov_v5tyq9woc().f[13]++;
    cov_v5tyq9woc().s[67]++;
    return async function securedHandler(request, context) {
      /* istanbul ignore next */
      cov_v5tyq9woc().f[14]++;
      cov_v5tyq9woc().s[68]++;
      // CORS检查
      if (config.allowedOrigins) {
        /* istanbul ignore next */
        cov_v5tyq9woc().b[16][0]++;
        const origin =
        /* istanbul ignore next */
        (cov_v5tyq9woc().s[69]++, request.headers.get('origin'));
        /* istanbul ignore next */
        cov_v5tyq9woc().s[70]++;
        if (
        /* istanbul ignore next */
        (cov_v5tyq9woc().b[18][0]++, origin) &&
        /* istanbul ignore next */
        (cov_v5tyq9woc().b[18][1]++, !config.allowedOrigins.includes(origin))) {
          /* istanbul ignore next */
          cov_v5tyq9woc().b[17][0]++;
          cov_v5tyq9woc().s[71]++;
          return server_1.NextResponse.json({
            error: 'Origin not allowed'
          }, {
            status: 403
          });
        } else
        /* istanbul ignore next */
        {
          cov_v5tyq9woc().b[17][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_v5tyq9woc().b[16][1]++;
      }
      // 请求大小检查
      cov_v5tyq9woc().s[72]++;
      if (config.maxRequestSize) {
        /* istanbul ignore next */
        cov_v5tyq9woc().b[19][0]++;
        const contentLength =
        /* istanbul ignore next */
        (cov_v5tyq9woc().s[73]++, request.headers.get('content-length'));
        /* istanbul ignore next */
        cov_v5tyq9woc().s[74]++;
        if (
        /* istanbul ignore next */
        (cov_v5tyq9woc().b[21][0]++, contentLength) &&
        /* istanbul ignore next */
        (cov_v5tyq9woc().b[21][1]++, parseInt(contentLength) > config.maxRequestSize)) {
          /* istanbul ignore next */
          cov_v5tyq9woc().b[20][0]++;
          cov_v5tyq9woc().s[75]++;
          return server_1.NextResponse.json({
            error: 'Request too large'
          }, {
            status: 413
          });
        } else
        /* istanbul ignore next */
        {
          cov_v5tyq9woc().b[20][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_v5tyq9woc().b[19][1]++;
      }
      // CSRF检查
      cov_v5tyq9woc().s[76]++;
      if (config.enableCSRF) {
        /* istanbul ignore next */
        cov_v5tyq9woc().b[22][0]++;
        const csrfToken =
        /* istanbul ignore next */
        (cov_v5tyq9woc().s[77]++, request.headers.get('x-csrf-token'));
        /* istanbul ignore next */
        cov_v5tyq9woc().s[78]++;
        if (!csrfToken) {
          /* istanbul ignore next */
          cov_v5tyq9woc().b[23][0]++;
          cov_v5tyq9woc().s[79]++;
          return server_1.NextResponse.json({
            error: 'CSRF token required'
          }, {
            status: 403
          });
        } else
        /* istanbul ignore next */
        {
          cov_v5tyq9woc().b[23][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_v5tyq9woc().b[22][1]++;
      }
      cov_v5tyq9woc().s[80]++;
      return await handler(request, context);
    };
  };
}
// 📝 命名规范：组合中间件工具
function combineMiddleware(...middlewares) {
  /* istanbul ignore next */
  cov_v5tyq9woc().f[15]++;
  cov_v5tyq9woc().s[81]++;
  return function combinedMiddleware(handler) {
    /* istanbul ignore next */
    cov_v5tyq9woc().f[16]++;
    cov_v5tyq9woc().s[82]++;
    return middlewares.reduceRight((acc, middleware) => {
      /* istanbul ignore next */
      cov_v5tyq9woc().f[17]++;
      cov_v5tyq9woc().s[83]++;
      return middleware(acc);
    }, handler);
  };
}
// 📝 命名规范：使用示例导出
/* istanbul ignore next */
cov_v5tyq9woc().s[84]++;
exports.exampleUsage = {
  // 基础验证示例
  basicValidation: withValidation({
    validateBody: zod_1.z.object({
      name: zod_1.z.string().min(1),
      email: zod_1.z.string().email()
    })
  }),
  // 组合中间件示例
  fullProtection: combineMiddleware(withSecurity({
    enableCSRF: true,
    enableRateLimit: true,
    allowedOrigins: ['https://yourdomain.com'],
    maxRequestSize: 10 * 1024 * 1024 // 10MB
  }), exports.validateRequest.userAuth({
    debug: process.env.NODE_ENV === 'development'
  }))
};
