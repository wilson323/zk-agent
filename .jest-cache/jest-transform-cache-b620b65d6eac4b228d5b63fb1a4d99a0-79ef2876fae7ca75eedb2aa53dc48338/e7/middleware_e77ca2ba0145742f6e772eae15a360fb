      validateBody: zod_1.z.object({
        email: zod_1.z.string().email(),
        password: zod_1.z.string().min(8),
        name: zod_1.z.string().min(1).max(100).optional()
      }),
      ...config
    });
  },
  // 智能体配置验证
  agentConfig: config => {
    /* istanbul ignore next */
    cov_v5tyq9woc().f[11]++;
    cov_v5tyq9woc().s[65]++;
