      validateBody: zod_1.z.object({
        id: zod_1.z.string().regex(/^[a-zA-Z0-9_-]+$/),
        name: zod_1.z.string().min(1).max(50),
        type: zod_1.z.enum(['fastgpt', 'cad', 'poster', 'custom']),
        description: zod_1.z.string().max(500).optional(),
        capabilities: zod_1.z.array(zod_1.z.string()).max(20).default([]),
        config: zod_1.z.record(zod_1.z.unknown()).default({}),
        enabled: zod_1.z.boolean().default(true),
