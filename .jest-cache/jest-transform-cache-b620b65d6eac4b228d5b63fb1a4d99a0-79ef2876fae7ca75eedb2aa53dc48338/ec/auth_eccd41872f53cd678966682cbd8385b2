/**
 * 验证刷新令牌
 */
function verifyRefreshToken(token) {
  /* istanbul ignore next */
  cov_puvnz9hls().f[15]++;
  cov_puvnz9hls().s[72]++;
  try {
    const decoded =
    /* istanbul ignore next */
    (cov_puvnz9hls().s[73]++, jsonwebtoken_1.default.verify(token, env_1.jwtConfig.secret));
    /* istanbul ignore next */
    cov_puvnz9hls().s[74]++;
    if (
    /* istanbul ignore next */
    (cov_puvnz9hls().b[26][0]++, decoded.type !== 'refresh') ||
    /* istanbul ignore next */
    (cov_puvnz9hls().b[26][1]++, !decoded.userId)) {
      /* istanbul ignore next */
      cov_puvnz9hls().b[25][0]++;
      cov_puvnz9hls().s[75]++;
