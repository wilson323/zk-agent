/**
 * 从请求中提取JWT令牌
 */
function extractToken(req) {
  /* istanbul ignore next */
  cov_puvnz9hls().f[12]++;
  // 从 Authorization header 中提取
  const authHeader =
  /* istanbul ignore next */
  (cov_puvnz9hls().s[60]++, req.headers.get('authorization'));
  /* istanbul ignore next */
  cov_puvnz9hls().s[61]++;
  if (
  /* istanbul ignore next */
  (cov_puvnz9hls().b[21][0]++, authHeader) &&
  /* istanbul ignore next */
  (cov_puvnz9hls().b[21][1]++, authHeader.startsWith('Bearer '))) {
    /* istanbul ignore next */
    cov_puvnz9hls().b[20][0]++;
    cov_puvnz9hls().s[62]++;
    return authHeader.substring(7);
  } else
  /* istanbul ignore next */
  {
    cov_puvnz9hls().b[20][1]++;
  }
  // 从 Cookie 中提取
  const tokenCookie =
  /* istanbul ignore next */
  (cov_puvnz9hls().s[63]++, req.cookies.get('auth-token'));
  /* istanbul ignore next */
  cov_puvnz9hls().s[64]++;
  if (tokenCookie) {
    /* istanbul ignore next */
    cov_puvnz9hls().b[22][0]++;
    cov_puvnz9hls().s[65]++;
