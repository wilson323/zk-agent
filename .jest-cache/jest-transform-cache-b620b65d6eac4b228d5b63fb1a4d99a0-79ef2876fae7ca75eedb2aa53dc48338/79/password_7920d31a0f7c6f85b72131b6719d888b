 * 生成随机密码
 */
function generateRandomPassword(length =
/* istanbul ignore next */
(cov_4xnqbsguc().b[15][0]++, 12)) {
  /* istanbul ignore next */
  cov_4xnqbsguc().f[4]++;
  const lowercase =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[48]++, "abcdefghijklmnopqrstuvwxyz");
  const uppercase =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[49]++, "ABCDEFGHIJKLMNOPQRSTUVWXYZ");
  const numbers =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[50]++, "0123456789");
  const symbols =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[51]++, "!@#$%^&*()_+-=[]{}|;:,.<>?");
  const allChars =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[52]++, lowercase + uppercase + numbers + symbols);
  let password =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[53]++, "");
  // 确保至少包含每种字符类型
  /* istanbul ignore next */
  cov_4xnqbsguc().s[54]++;
  password += lowercase[Math.floor(Math.random() * lowercase.length)];
  /* istanbul ignore next */
  cov_4xnqbsguc().s[55]++;
  password += uppercase[Math.floor(Math.random() * uppercase.length)];
  /* istanbul ignore next */
  cov_4xnqbsguc().s[56]++;
  password += numbers[Math.floor(Math.random() * numbers.length)];
  /* istanbul ignore next */
  cov_4xnqbsguc().s[57]++;
  password += symbols[Math.floor(Math.random() * symbols.length)];
  // 填充剩余长度
  /* istanbul ignore next */
  cov_4xnqbsguc().s[58]++;
  for (let i =
  /* istanbul ignore next */
  (cov_4xnqbsguc().s[59]++, 4); i < length; i++) {
    /* istanbul ignore next */
    cov_4xnqbsguc().s[60]++;
    password += allChars[Math.floor(Math.random() * allChars.length)];
  }
  // 打乱字符顺序
  /* istanbul ignore next */
  cov_4xnqbsguc().s[61]++;
  return password.split("").sort(() => {
    /* istanbul ignore next */
    cov_4xnqbsguc().f[5]++;
    cov_4xnqbsguc().s[62]++;
    return Math.random() - 0.5;
  }).join("");
}
