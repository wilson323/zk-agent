      (cov_2jtli5tiv6().s[53]++, ctx.measureText(testLine));
      /* istanbul ignore next */
      cov_2jtli5tiv6().s[54]++;
      if (
      /* istanbul ignore next */
      (cov_2jtli5tiv6().b[9][0]++, metrics.width > maxWidth) &&
      /* istanbul ignore next */
      (cov_2jtli5tiv6().b[9][1]++, line !== "")) {
        /* istanbul ignore next */
        cov_2jtli5tiv6().b[8][0]++;
        cov_2jtli5tiv6().s[55]++;
        ctx.fillText(line, 20, lineY);
        /* istanbul ignore next */
        cov_2jtli5tiv6().s[56]++;
        line = word + " ";
        /* istanbul ignore next */
        cov_2jtli5tiv6().s[57]++;
        lineY += 24;
      } else {
        /* istanbul ignore next */
        cov_2jtli5tiv6().b[8][1]++;
        cov_2jtli5tiv6().s[58]++;
        line = testLine;
      }
    }
    /* istanbul ignore next */
    cov_2jtli5tiv6().s[59]++;
    ctx.fillText(line, 20, lineY);
    // 绘制时间戳
    /* istanbul ignore next */
    cov_2jtli5tiv6().s[60]++;
    if (message.timestamp) {
      /* istanbul ignore next */
      cov_2jtli5tiv6().b[10][0]++;
      const date =
      /* istanbul ignore next */
      (cov_2jtli5tiv6().s[61]++, new Date(message.timestamp));
      const timeStr =
      /* istanbul ignore next */
      (cov_2jtli5tiv6().s[62]++, date.toLocaleString());
      /* istanbul ignore next */
      cov_2jtli5tiv6().s[63]++;
      ctx.font = '14px "Noto Sans SC"';
      /* istanbul ignore next */
      cov_2jtli5tiv6().s[64]++;
      ctx.fillStyle = "#999999";
      /* istanbul ignore next */
      cov_2jtli5tiv6().s[65]++;
      ctx.textAlign = "right";
      /* istanbul ignore next */
      cov_2jtli5tiv6().s[66]++;
      ctx.fillText(timeStr, width - 20, y + messageHeight - 20);
    } else
    /* istanbul ignore next */
    {
      cov_2jtli5tiv6().b[10][1]++;
    }
    cov_2jtli5tiv6().s[67]++;
    y += messageHeight;
  }
  // 保存图片到临时文件
  const tempDir =
  /* istanbul ignore next */
  (cov_2jtli5tiv6().s[68]++, path_1.default.join(os_1.default.tmpdir(), "zkteco-chat-images"));
  /* istanbul ignore next */
  cov_2jtli5tiv6().s[69]++;
  await promises_1.default.mkdir(tempDir, {
    recursive: true
  });
  const fileName =
  /* istanbul ignore next */
  (cov_2jtli5tiv6().s[70]++, `chat_${Date.now()}.png`);
  const filePath =
  /* istanbul ignore next */
  (cov_2jtli5tiv6().s[71]++, path_1.default.join(tempDir, fileName));
  const buffer =
  /* istanbul ignore next */
  (cov_2jtli5tiv6().s[72]++, canvas.toBuffer("image/png"));
  /* istanbul ignore next */
  cov_2jtli5tiv6().s[73]++;
  await promises_1.default.writeFile(filePath, buffer);
  // 返回图片URL
  /* istanbul ignore next */
  cov_2jtli5tiv6().s[74]++;
  return {
    imageUrl: `/api/images/temp/${fileName}`
  };
}
