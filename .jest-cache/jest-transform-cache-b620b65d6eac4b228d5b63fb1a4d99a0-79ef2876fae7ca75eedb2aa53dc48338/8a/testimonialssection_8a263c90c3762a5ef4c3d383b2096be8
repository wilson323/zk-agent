const TestimonialsSection =
/* istanbul ignore next */
(cov_ylipn24jq().s[30]++, (0, react_1.memo)(() => {
  /* istanbul ignore next */
  cov_ylipn24jq().f[9]++;
  const [currentIndex, setCurrentIndex] =
  /* istanbul ignore next */
  (cov_ylipn24jq().s[31]++, (0, react_1.useState)(0));
  const [isAutoPlaying, setIsAutoPlaying] =
  /* istanbul ignore next */
  (cov_ylipn24jq().s[32]++, (0, react_1.useState)(true));
  // 自动轮播
  /* istanbul ignore next */
  cov_ylipn24jq().s[33]++;
  (0, react_1.useEffect)(() => {
    /* istanbul ignore next */
    cov_ylipn24jq().f[10]++;
    cov_ylipn24jq().s[34]++;
    if (!isAutoPlaying) {
      /* istanbul ignore next */
      cov_ylipn24jq().b[7][0]++;
      cov_ylipn24jq().s[35]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ylipn24jq().b[7][1]++;
    }
    const interval =
    /* istanbul ignore next */
    (cov_ylipn24jq().s[36]++, setInterval(() => {
      /* istanbul ignore next */
      cov_ylipn24jq().f[11]++;
      cov_ylipn24jq().s[37]++;
      setCurrentIndex(prev => {
        /* istanbul ignore next */
        cov_ylipn24jq().f[12]++;
        cov_ylipn24jq().s[38]++;
        return (prev + 1) % testimonialsData.length;
      });
    }, 5000));
    /* istanbul ignore next */
    cov_ylipn24jq().s[39]++;
    return () => {
      /* istanbul ignore next */
      cov_ylipn24jq().f[13]++;
      cov_ylipn24jq().s[40]++;
      return clearInterval(interval);
    };
  }, [isAutoPlaying]);
  const handlePrevious =
  /* istanbul ignore next */
  (cov_ylipn24jq().s[41]++, (0, react_1.useCallback)(() => {
    /* istanbul ignore next */
    cov_ylipn24jq().f[14]++;
    cov_ylipn24jq().s[42]++;
    setCurrentIndex(prev => {
      /* istanbul ignore next */
      cov_ylipn24jq().f[15]++;
      cov_ylipn24jq().s[43]++;
      return prev === 0 ?
      /* istanbul ignore next */
      (cov_ylipn24jq().b[8][0]++, testimonialsData.length - 1) :
      /* istanbul ignore next */
      (cov_ylipn24jq().b[8][1]++, prev - 1);
    });
    /* istanbul ignore next */
    cov_ylipn24jq().s[44]++;
    setIsAutoPlaying(false);
  }, []));
  const handleNext =
  /* istanbul ignore next */
  (cov_ylipn24jq().s[45]++, (0, react_1.useCallback)(() => {
    /* istanbul ignore next */
    cov_ylipn24jq().f[16]++;
    cov_ylipn24jq().s[46]++;
    setCurrentIndex(prev => {
      /* istanbul ignore next */
      cov_ylipn24jq().f[17]++;
      cov_ylipn24jq().s[47]++;
      return (prev + 1) % testimonialsData.length;
    });
    /* istanbul ignore next */
    cov_ylipn24jq().s[48]++;
    setIsAutoPlaying(false);
  }, []));
  const handleDotClick =
  /* istanbul ignore next */
  (cov_ylipn24jq().s[49]++, (0, react_1.useCallback)(index => {
    /* istanbul ignore next */
