    for (let i =
    /* istanbul ignore next */
    (cov_ylipn24jq().s[55]++, 0); i < 3; i++) {
      const index =
      /* istanbul ignore next */
      (cov_ylipn24jq().s[56]++, (currentIndex + i) % testimonialsData.length);
      /* istanbul ignore next */
      cov_ylipn24jq().s[57]++;
      visible.push({
        ...testimonialsData[index],
        isActive: i === 1 // 中间的卡片为激活状态
      });
    }
    /* istanbul ignore next */
    cov_ylipn24jq().s[58]++;
