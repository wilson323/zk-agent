    className: "space-y-8",
    children: [(0, jsx_runtime_1.jsxs)("div", {
      className: "relative",
      children: [(0, jsx_runtime_1.jsx)("div", {
        className: "grid grid-cols-1 md:grid-cols-3 gap-6",
        children: getVisibleTestimonials().map((testimonial, index) => {
          /* istanbul ignore next */
