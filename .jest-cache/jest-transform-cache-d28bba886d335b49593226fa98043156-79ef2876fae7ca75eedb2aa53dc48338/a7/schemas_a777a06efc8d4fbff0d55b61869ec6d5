const validateCADFile = (file)=>{
    try {
        const data = CADFileSchema.parse(file);
        return {
            success: true,
            data
        };
    } catch (error) {
        if (error instanceof _zod.z.ZodError) {
            return {
                success: false,
                error: {
                    message: 'CADÊñá‰ª∂È™åËØÅÂ§±Ë¥•',
                    issues: error.issues.map((issue)=>({
                            path: issue.path,
                            message: issue.message,
                            code: issue.code
                        }))
                }
            };
        }
        return {
            success: false,
            error: {
                message: 'Êú™Áü•È™åËØÅÈîôËØØ',
                issues: []
            }
        };
    }
};
